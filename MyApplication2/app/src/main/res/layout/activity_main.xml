<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".review_ly">

        <LinearLayout
            android:id="@+id/linearLayout12"
            android:layout_width="356dp"
            android:layout_height="50dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="47dip">

                <Button
                    android:id="@+id/Button"
                    android:layout_width="18dip"
                    android:layout_height="18dip"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="10dip"
                    android:background="@mipmap/menu_01">


                </Button>

                <Button
                    android:layout_width="18dip"
                    android:layout_height="18dip"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="10dip"
                    android:background="@mipmap/search_01">


                </Button>

                <TextView
                    android:id="@+id/tv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="레스토랑"
                    android:textColor="@color/color_333333"
                    android:textSize="18dip"
                    android:textStyle="bold">


                </TextView>


            </FrameLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout13"
            android:layout_width="401dp"
            android:layout_height="41dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="9dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout12">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="201dp"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/textView33"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="메뉴"
                        android:textSize="25sp" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="177dp"
                    android:layout_height="43dp">

                    <TextView
                        android:id="@+id/textView34"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="리뷰"
                        android:textSize="25sp" />
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout14"
            android:layout_width="390dp"
            android:layout_height="92dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="10dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout13">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="195dp"
                    android:layout_height="match_parent">

                    <ImageButton
                        android:id="@+id/imageButton12"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/line" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="202dp"
                    android:layout_height="85dp">

                    <ImageButton
                        android:id="@+id/imageButton13"
                        android:layout_width="185dp"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/line_off" />
                </FrameLayout>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout15"
            android:layout_width="368dp"
            android:layout_height="133dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="10dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout14">

            <LinearLayout
                android:layout_width="395dp"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_gravity="center">

                    <ImageView
                        android:id="@+id/imageView13"
                        android:layout_width="90dp"
                        android:layout_height="75dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/piz" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="137dp"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="64dp">

                            <TextView
                                android:id="@+id/textView35"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="bottom|start"
                                android:text="  페퍼로니 피자"
                                android:textSize="18sp" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/textView36"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="  13,900원"
                                android:textSize="18sp" />
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="35dp"
                    android:layout_height="match_parent">

                    <ImageButton
                        android:id="@+id/imageButton15"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/down" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="39dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/textView37"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="1"
                        android:textSize="28sp" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="32dp"
                    android:layout_height="match_parent">

                    <ImageButton
                        android:id="@+id/imageButton16"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/up" />
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearLayout16"
            android:layout_width="366dp"
            android:layout_height="132dp"
            android:layout_marginStart="23dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="22dp"
            android:orientation="vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout15">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="90dp"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView14"
                        android:layout_width="70dp"
                        android:layout_height="79dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/piza1" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="136dp"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="60dp">

                            <TextView
                                android:id="@+id/textView38"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:gravity="bottom|start"
                                android:text="  토마토 피자"
                                android:textSize="18sp" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <TextView
                                android:id="@+id/textView39"
                                android:layout_width="match_parent"
                                android:layout_height="56dp"
                                android:text="  12,900원"
                                android:textSize="18sp" />
                        </FrameLayout>
                    </LinearLayout>
                </FrameLayout>

                <FrameLayout
                    android:layout_width="34dp"
                    android:layout_height="match_parent">

                    <ImageButton
                        android:id="@+id/imageButton17"
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/down" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="38dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/textView40"
                        android:layout_width="match_parent"
                        android:layout_height="120dp"
                        android:gravity="center"
                        android:text="1"
                        android:textSize="27sp" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="33dp"
                    android:layout_height="match_parent">

                    <ImageButton
                        android:id="@+id/imageButton18"
                        android:layout_width="25sp"
                        android:layout_height="25sp"
                        android:layout_gravity="center"
                        app:srcCompat="@drawable/up" />
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="370dp"
            android:layout_height="124dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="75dp"
            android:layout_marginEnd="10dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <FrameLayout
                    android:layout_width="274dp"
                    android:layout_height="59dp">

                    <Button
                        android:id="@+id/button3"
                        android:layout_width="match_parent"
                        android:layout_height="73dp"
                        android:backgroundTint="@android:color/background_dark"
                        android:text="주문완료"
                        android:textColor="@android:color/background_light"
                        android:textSize="20sp" />
                </FrameLayout>

                <FrameLayout
                    android:layout_width="95dp"
                    android:layout_height="59dp">

                    <Button
                        android:id="@+id/button4"
                        android:layout_width="wrap_content"
                        android:layout_height="71dp"
                        android:text="예약"
                        android:textSize="22sp" />
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
